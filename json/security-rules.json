{
  "rules": {
    "users": {
      ".write": "auth !== null",
      ".read": false
    },
    "feedbackplanet": {
      ".write": "root.child('users').hasChild(auth.uid)",
      ".read": false,
      "$uid": {
        ".read": "auth !== null && auth.uid === $uid",
        ".write": "auth !== null && auth.uid === $uid"
      }
    }
  }
}
